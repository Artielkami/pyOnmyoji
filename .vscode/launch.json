    "version": "0.2.0",
    "configurations": [
        {
            "name": "run in powershell",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}\\onmyoji.ps1",
            "cwd": "${file}"
        },
        {
            "name": "gui",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\ui.py",
            "console": "integratedTerminal"
        },
        {
            "name": "match test from screenshot",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\match_test.py",
            "console": "integratedTerminal",
            "args": ["screenshot"]
        },
        {
            "name": "match test from file",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\match_test.py",
            "console": "integratedTerminal",
            "args": ["file"]
        },
        {
            "name": "baiguiyexing",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\onmyoji.py",
            "console": "integratedTerminal",
            "args": ["baiguiyexing"]
        },
        {
            "name": "yuhun_member",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\onmyoji.py",
            "console": "integratedTerminal",
            "args": ["-t 5","yuhun_member"]
        },
        {
            "name": "yuhun_leader",
            "type": "python",
